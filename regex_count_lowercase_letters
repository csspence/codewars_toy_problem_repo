/*
Your task is simply to count the total number of lowercase letters in a string.
*/

const lowercaseCount = (str) => {
  let counter = 0;
  let letters = 'abcdefghijklmnopqrstuvwxyz'
  for(let i = 0; i < str.length; i++) {
    if(letters.includes(str[i])) {
      counter++;
    }
  }
  
  return counter;
}
